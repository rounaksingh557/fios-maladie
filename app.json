{
  "expo": {
    "name": "fios-maladie",
    "slug": "fios-maladie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Image/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rounakexpo.fiosmaladie",
      "googleServicesFile": "./SSL/IOS-Client-Id.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Image/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rounakexpo.fiosmaladie",
      "googleServicesFile": "./SSL/Android-Client-Id.json"
    },
    "web": {
      "favicon": "./assets/Image/logo.png"
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    }
  }
}
